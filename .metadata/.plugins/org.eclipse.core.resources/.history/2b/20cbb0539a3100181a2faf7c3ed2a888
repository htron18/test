
public class HelloWorld {

	public static void main(String[] args) {
		//HelloWorldの出力
		System.out.println("Hello World");
		
		char char1='A';
		System.out.println(char1);
		
		int a=10;
		String b = a>=0?"プラス":"マイナス";
		System.out.println(b);
		
		for(int i=1;i<=5;i++){
			String c=i%2==0?"★":"☆";
			System.out.println(c);
		}
	}
	

}
