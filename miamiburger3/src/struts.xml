<?xml version="1.0" encoding="UTF-8"?>

 <!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
<constant name="struts.devMode" value="true" />
<constant name="struts.ui.theme" value="simple" />
<package name="com.internousdev.miamiburger3.action" extends="struts-default">

	<action name="LoginAction" class="com.internousdev.miamiburger3.action.GoLoginAction" method="execute">
 		<result name="success">login.jsp</result>
	</action>

	<action name="GoLoginAction" class="com.internousdev.miamiburger3.action.LoginAction" method="execute">
 		<result name="success">home.jsp</result>
		<result name="error">login.jsp</result>
		<result name="input">master.jsp</result>
	</action>

</package>
</struts>